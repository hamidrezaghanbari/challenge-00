{
    "env": {
        "production": {
            "plugins": [
                [
                    "react-remove-properties",
                    {
                        "properties": [
                            "data-test"
                        ]
                    }
                ]
            ]
        }
    },
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "styled-components",
            {
                "ssr": true,
                "displayName": true,
                "preprocess": false
            }
        ]
    ]
}